		"babel-runtime": "^6.26.0",
		"classnames": "^2.2.6",
		"css-loader": "^0.28.11",
		"node-sass": "^4.10.0",
		"onchange": "^5.1.0",
		"prettier": "^1.15.2",
		"sass-loader": "^7.1.0",
